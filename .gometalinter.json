{
  "Enable": [
    "structcheck",
    "maligned",
    "nakedret",
    "deadcode",
    "gocyclo",
    "ineffassign",
    "dupl",
    "golint",
    "gotype",
    "goimports",
    "varcheck",
    "interfacer",
    "goconst",
    "unparam",
    "megacheck",
    "misspell",
    "lll",
    "gas",
    "safesql"
