{
  "Linters": {
    "megacheck_provider": {
      "Command": "megacheck -unused.enabled=false",
      "Pattern": "PATH:LINE:COL:MESSAGE"
    }
  },
  "DisableAll": true,
  "Enable": [
    "structcheck",
    "maligned",
    "nakedret",
    "deadcode",
    "gocyclo",
    "ineffassign",
    "dupl",
    "golint",
    "gotype",
    "goimports",
    "varcheck",
    "interfacer",
    "goconst",
    "unparam",
    "misspell",
    "lll",
    "gas",
    "safesql"
  ],
  "Vendor": true,
  "LineLength": 120,
  "Exclude": [
    "fake_([a-zA-Z]+)",
    "mocks",
    "([a-zA-Z]+)_test.go",
    "([a-zA-Z]+)_enumer.go",
    "statik",
    "public"
  ],
  "Deadline": "2m",
  "Cyclo": 13
}